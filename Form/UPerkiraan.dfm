  object qry_perkiraan: TADOQuery
    ConnectionString = 
      'Provider=MSDASQL.1;Persist Security Info=False;User ID=user;Data' +
      ' Source=dasi_ntb'
    CursorType = ctStatic
    OnCalcFields = qry_perkiraanCalcFields
    Parameters = <
      item
        Name = 'periode'
        Attributes = [paNullable]
        DataType = ftString
        Precision = 255
        Size = 255
        Value = '2013-07'
      end>
    SQL.Strings = (
      'select  p.kd_kel, k.nama_kel as nama_kelompok, '
      'if(level='#39'Detail'#39',p.kd_header,'#39'-1'#39') as kd_header, p.prioritas,'
      
        'p.id_perkiraan, p.nama_perkiraan, p.level,k.jenis, sp.saldo_awal' +
        ', p.singkatan,'
      'spe.saldo_penyaluran,'
      'p.alokasi_amil '
      
        'from tbl_perkiraan p inner join tbl_kelompok k on p.kd_kel=k.kd_' +
        'kel '
      'left join tbl_saldo_periode sp '
      'on (sp.id_perkiraan=p.id_perkiraan and sp.periode=:periode )'
      'left join tbl_saldo_penyaluran spe'
      'on(sp.id_perkiraan=spe.id_perkiraan)'
      'order by p.prioritas,k.kd_kel,p.id_perkiraan')
    Left = 304
    Top = 304
