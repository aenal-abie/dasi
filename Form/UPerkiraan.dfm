  object qryHeader: TADOQuery
    ConnectionString = 
      'Provider=MSDASQL.1;Persist Security Info=False;User ID=user;Data' +
      ' Source=dasi_ntb'
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'kd_kel'
        Attributes = [paNullable]
        DataType = ftString
        Precision = 255
        Size = 255
        Value = Null
      end>
    SQL.Strings = (
      'SELECT'
      'tbl_perkiraan.id_perkiraan,'
      'tbl_perkiraan.nama_perkiraan,'
      'tbl_kelompok.jenis'
      'FROM'
      'tbl_perkiraan'
      
        'INNER JOIN tbl_kelompok ON tbl_perkiraan.kd_kel = tbl_kelompok.k' +
        'd_kel'
      'WHERE'
      'tbl_perkiraan.`level` = '#39'Header'#39' '
      'AND'
      'tbl_perkiraan.kd_kel =:kd_kel')
    Left = 448
    Top = 309
