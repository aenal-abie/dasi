  object qryMutasi: TADOQuery
    Tag = 1
    ConnectionString = 
      'Provider=MSDASQL.1;Persist Security Info=False;User ID=user;Data' +
      ' Source=dasi_ntb'
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT'
      'p2.nama_perkiraan AS tujuan,'
      'p1.nama_perkiraan AS sumber,'
      'op1.*'
      'FROM'
      'tbl_peminjaman_operasional AS op1'
      'INNER JOIN tbl_perkiraan AS p1 ON p1.id_perkiraan = op1.dari'
      'INNER JOIN tbl_perkiraan AS p2 ON p2.id_perkiraan = op1.ke')
    Left = 728
    Top = 108
  end
