  object qry_perkiraan: TADOQuery
    Active = True
    ConnectionString = 
      'Provider=MSDASQL.1;Persist Security Info=False;User ID=root;Data' +
      ' Source=dasi_ntb'
    CursorType = ctStatic
    OnCalcFields = qry_perkiraanCalcFields
    Parameters = <>
    SQL.Strings = (
      'SELECT'
      's.saldo_awal,'
      'p.nama_perkiraan,'
      'p.id_perkiraan,'
      'k.jenis,'
      'k.nama_kel,'
      'k.kd_kel,'
      'p.`level`'
      'FROM'
      'tbl_saldo_periode AS s'
      'INNER JOIN tbl_perkiraan AS p ON s.id_perkiraan = p.id_perkiraan'
      'INNER JOIN tbl_kelompok AS k ON k.kd_kel = p.kd_kel')
